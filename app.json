{
  "expo": {
    "name": "app-utilitario",
    "slug": "app-utilitario",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/iphone-preview-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.igorbaio.apputilitario"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000"
      },
      "userInterfaceStyle": "light",
      "package": "com.igorbaio.apputilitario"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
